{
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "akamud",
    "arthaud",
    "autofix",
    "bioinformaticist",
    "Bioinformaticists",
    "budparr",
    "cartoonishly",
    "chrischinchilla",
    "codespell",
    "creedenzymatic",
    "Desmoplasia",
    "devicon",
    "ecmel",
    "esbenp",
    "Imamiland",
    "IPTM",
    "isset",
    "Kinome",
    "linebreak",
    "markdownlint",
    "Moana",
    "Moffat",
    "Neuropsychopharmacology",
    "Omics",
    "onedark",
    "pocketwatch",
    "Prometric",
    "proselint",
    "raison",
    "Repurpos",
    "Rxiv",
    "Sajid",
    "scminput",
    "Transcriptomic",
    "USMLE",
    "Whvp",
    "wordmark"
  ],
  "editor.fontFamily": "FiraCode Nerd Font Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.rulers": [
    80,
    120
  ],
  "editor.wordWrap": "on",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "scminput": false
  },
  "markdown.editor.filePaste.enabled": "smart",
  "markdown.editor.pasteUrlAsFormattedLink.enabled": "smart",
  "markdown.math.enabled": true,
  "workbench.colorTheme": "Catppuccin Macchiato",
  "workbench.editor.showTabs": "multiple"
}
